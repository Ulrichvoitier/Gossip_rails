<h1>Pour accedera votre session merci de vous identifier !</h1>
<p>Find me in app/views/inscription/inscription_section.html.erb</p>

<html>
  <body>

    <%= form_tag url_for(action: 'create'), method: "post" do %>
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
      
      <%= label_tag 'Last_name' %>
      <%= text_field_tag 'user[last_name]' %>
      <br /> <br />

      <%= label_tag 'Email' %>
      <%= text_field_tag 'user[email]' %>
      <br /> <br />

      <%= label_tag 'Password' %>
      <%= text_area_tag 'user[password]' %>
      <br /> <br />

      <%= submit_tag "Connexion" %>

    <% end %>

  </body>
</html>